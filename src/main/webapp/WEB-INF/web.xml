<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<listener>
		<listener-class>it.engineering.web.zadatak.listeners.MainContextListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>DispatchServlet</servlet-name>
		<display-name>DispatchServlet</display-name>
		<description></description>
		<servlet-class>it.engineering.web.zadatak.servlets.DispatchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DispatchServlet</servlet-name>
		<url-pattern>/app/*</url-pattern>
	</servlet-mapping>
</web-app>
